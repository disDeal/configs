[user]
	email = k.a.komissar@gmail.com
	name = Alex Komissarov
	signingkey = 601D1BCA551520C017510C3085438EEA69128CC4
[ui "color"]
	ui = always
[color]
	ui = true
[alias]
    tree = log --graph --decorate --pretty=oneline --abbrev-commit
	c = commit
	s = status
	st = status
	lg = log --oneline
	last = log -1 HEAD
	f = push --force-with-lease
	# https://stackoverflow.com/a/11688523/472927
	ss = "!f() { git stash show stash^{/$*} -p; }; f"
	sa = "!f() { git stash apply stash^{/$*}; }; f"
	sl = stash list
[push]
	default = simple
[help]
	autocorrect = 1
[github]
	user = disDeal
[diff]
	algorithm = patience
	compactionHeuristic = true
# [commit]
#        gpgsign = true
[protocol]
	version = 2
